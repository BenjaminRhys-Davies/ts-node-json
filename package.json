  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.22",
    "@types/progress": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "graceful-fs": "^4.2.4",
    "progress": "^2.0.3"
